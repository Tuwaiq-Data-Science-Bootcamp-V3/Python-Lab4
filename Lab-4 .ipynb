    "#help resource       \n",
    "#https://pynative.com/print-pattern-python-examples/  \n",
    "\n",
    "def desNum(x):\n",
    "    for i in range(x,0,-1):\n",
    "        for y in range (i,0,-1):\n",
    "            print(y,end=\" \")\n",
    "        print(\"\\r\")\n",
    "x=int(input(\"add number\"))\n",
    "desNum(x) \n",
