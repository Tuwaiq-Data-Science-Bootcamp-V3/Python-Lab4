    "def print_(n) :#\n",
    "    for i in range(n, 0,-1):# Start,Stop,step  used -1 to sub the numbers\n",
    "        for j in range(i, 0,-1):\n",
    "            print(j, end=\" \") #End to make space between numbers\n",
    "        print()# new line\n",
    "        \n",
    "\n",
    "        \n",
    "print_(5)"
   ]
